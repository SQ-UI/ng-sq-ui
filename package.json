{
  "name": "sq-ui",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "rimraf dist && ng serve --open",
    "build": "ng build",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage --reporters=coverage-istanbul",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "docsify serve docs",
    "copy:styles": "copyfiles -f \"./projects/ng-sq-common/src/lib/styles/**/*.scss\" \"./dist/ng-sq-common/styles\" && copyfiles -f \"./projects/ng-sq-common/src/lib/sq-ui-theme.scss\" \"./dist/ng-sq-common/\" && copyfiles -f \"./projects/ng-sq-ui/src/lib/styles/**/*.scss\" \"./dist/ng-sq-ui/styles\"",
    "build:libs": "ng build ng-sq-common && ng build ng-modal && ng build ng-datetime-picker && ng build ng-datatable && npm run copy:styles && ng build ng-sq-ui",
    "build:app:prod": "npm run build:libs && ng build --prod",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/forms": "^8.1.1",
    "@angular/platform-browser": "^8.1.1",
    "@angular/platform-browser-dynamic": "^8.1.1",
    "@angular/router": "^8.1.1",
    "all-contributors-cli": "^6.9.3",
    "core-js": "^3.3.2",
    "docsify": "^4.9.4",
    "nyc": "^14.1.1",
    "rxjs": "^6.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.14",
    "@angular-devkit/build-ng-packagr": "~0.803.9",
    "@angular/cli": "~8.3.9",
    "@angular/compiler-cli": "^8.1.1",
    "@angular/language-service": "^8.2.12",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/jasmine": "~3.4.4",
    "@types/node": "~12.7.12",
    "adm-zip": "^0.4.13",
    "codecov.io": "^0.1.6",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.1.1",
    "hawk": "^7.0.10",
    "hoek": "^6.1.3",
    "immutable": "^4.0.0-rc.12",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "marked": "^0.7.0",
    "mime": "^2.4.4",
    "moment": "^2.24.0",
    "ng-packagr": "^5.7.0",
    "protractor": "~5.4.2",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "ts-node": "~8.4.1",
    "tsickle": "^0.36.0",
    "tslib": "^1.10.0",
    "tslint": "~5.20.0",
    "tunnel-agent": "^0.6.0",
    "typescript": "^3.4.5"
  }
}
