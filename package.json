{
  "name": "sq-ui",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage --reporters=coverage-istanbul",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "docsify serve docs",
    "copy:readmes": "copyfiles -f \"./projects/ng-sq-common/src/README.md\" \"./dist/ng-sq-common/\" && copyfiles -f \"./projects/ng-modal/src/README.md\" \"./dist/ng-modal/\" && copyfiles -f \"./projects/ng-datetime-picker/src/README.md\" \"./dist/ng-datetime-picker/\" && copyfiles -f \"./projects/ng-datatable/src/README.md\" \"./dist/ng-datatable/\" && copyfiles -f \"./projects/ng-sq-ui/src/README.md\" \"./dist/ng-sq-ui/\"",
    "copy:styles": "copyfiles -f \"./projects/ng-sq-common/src/lib/styles/**/*.scss\" \"./dist/ng-sq-common/styles\" && copyfiles -f \"./projects/ng-sq-common/src/lib/sq-ui-theme.scss\" \"./dist/ng-sq-common/\" && copyfiles -f \"./projects/ng-sq-ui/src/lib/styles/**/*.scss\" \"./dist/ng-sq-ui/styles\"",
    "build:libs": "ng build ng-sq-common --prod && ng build ng-modal --prod && ng build ng-datetime-picker --prod && ng build ng-datatable --prod && ng build ng-sq-ui --prod && npm run copy:readmes && npm run copy:styles",
    "serve:libs": "npm run build:libs && ng serve --open",
    "build:app:prod": "npm run build:libs && ng build --prod",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.13",
    "@angular/common": "^7.2.13",
    "@angular/compiler": "^7.2.13",
    "@angular/core": "^7.2.13",
    "@angular/forms": "^7.2.13",
    "@angular/http": "^7.2.13",
    "@angular/platform-browser": "^7.2.13",
    "@angular/platform-browser-dynamic": "^7.2.13",
    "@angular/router": "^7.2.13",
    "all-contributors-cli": "^6.6.1",
    "docsify": "^4.9.4",
    "nyc": "^14.1.1",
    "rxjs": "^6.5.2",
    "core-js": "^3.1.3",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular-devkit/build-ng-packagr": "~0.13.9",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "^7.2.13",
    "@angular/language-service": "^7.2.15",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.0.2",
    "adm-zip": "^0.4.11",
    "codelyzer": "~5.0.1",
    "copyfiles": "^2.1.0",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "codecov.io": "^0.1.6",
    "karma": "4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "moment": "^2.24.0",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.2.0",
    "tsickle": "0.34.2",
    "tslib": "^1.9.0",
    "tslint": "~5.17.0",
    "typescript": "~3.2.4"
  }
}
