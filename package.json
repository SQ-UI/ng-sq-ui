  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "docsify serve docs",
    "copy:readmes": "copyfiles -f \"./projects/ng-sq-common/src/README.md\" \"./dist/ng-sq-common/\" && copyfiles -f \"./projects/ng-modal/src/README.md\" \"./dist/ng-modal/\" && copyfiles -f \"./projects/ng-datetime-picker/src/README.md\" \"./dist/ng-datetime-picker/\" && copyfiles -f \"./projects/ng-datatable/src/README.md\" \"./dist/ng-datatable/\" && copyfiles -f \"./projects/ng-sq-ui/src/README.md\" \"./dist/ng-sq-ui/\"",
    "build:libs": "ng build ng-sq-common --prod && ng build ng-modal --prod && ng build ng-datetime-picker --prod && ng build ng-datatable --prod && ng build ng-sq-ui --prod && npm run copy:readmes",
    "serve:libs": "npm run build:libs && ng serve --open",
    "build:app:prod": "npm run build:libs && ng build --prod",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "private": true,
