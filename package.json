{
  "name": "sq-ui",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "rimraf dist && ng serve --open",
    "build": "ng build",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage --reporters=coverage-istanbul",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "docsify serve docs",
    "copy:styles": "copyfiles -f \"./projects/ng-sq-common/src/lib/styles/**/*.scss\" \"./dist/ng-sq-common/styles\" && copyfiles -f \"./projects/ng-sq-common/src/lib/sq-ui-theme.scss\" \"./dist/ng-sq-common/\" && copyfiles -f \"./projects/ng-sq-ui/src/lib/styles/**/*.scss\" \"./dist/ng-sq-ui/styles\"",
    "build:libs": "ng build --prod ng-sq-common && ng build --prod ng-modal && ng build --prod ng-datetime-picker && ng build --prod ng-datatable && npm run copy:styles && ng build --prod ng-sq-ui",
    "build:app:prod": "npm run build:libs && ng build --prod",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.6",
    "@angular/common": "^9.0.6",
    "@angular/compiler": "^9.0.6",
    "@angular/core": "^9.0.6",
    "@angular/forms": "^9.0.6",
    "@angular/platform-browser": "^9.0.6",
    "@angular/platform-browser-dynamic": "^9.0.6",
    "@angular/router": "^9.0.6",
    "all-contributors-cli": "^6.19.0",
    "core-js": "^3.7.0",
    "docsify": "^4.11.6",
    "nyc": "^15.1.0",
    "rxjs": "^6.6.3",
    "tslib": "^1.14.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.6",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "^9.0.6",
    "@angular/language-service": "^9.1.7",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.7",
    "adm-zip": "^0.5.1",
    "codecov": "^3.8.1",
    "codelyzer": "^6.0.1",
    "copyfiles": "^2.4.0",
    "hawk": "^8.0.1",
    "hoek": "^6.1.3",
    "immutable": "^4.0.0-rc.12",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "marked": "^1.2.5",
    "mime": "^2.4.7",
    "moment": "^2.29.1",
    "ng-packagr": "^9.1.5",
    "protractor": "^7.0.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tunnel-agent": "^0.6.0",
    "typescript": "^3.7.5"
  }
}
