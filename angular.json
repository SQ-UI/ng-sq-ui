          }
        }
      }
    }
  },
  "defaultProject": "sq-ui",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "sq",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "sq"
    }
  }
}
